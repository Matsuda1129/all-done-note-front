{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/posts/[id].tsx", "pages/ajax.tsx", "pages/Todo.js", "pages/TodoList.js", "pages/App.js", "pages/user.tsx", "pages/reduxUser.tsx", "pages/_app.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
